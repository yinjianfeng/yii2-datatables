{
    "name": "qiubao/yii2-datatables",
    "description": "JQuery DataTables for Yii2",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "YinJianFeng",
            "email": "jeff@qiubaogogo.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "bower-asset/jquery":">= 1.7.0",
        "bower-asset/datatables":">= 1.9.4",
        "bower-asset/datatables-bootstrap3":"*",
        "bower-asset/datatables-tabletools":"*",
        "bower-asset/datatables-fixedcolumns":"*"
